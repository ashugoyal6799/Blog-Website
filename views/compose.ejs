<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/compose" method="POST" >
    <div class="form-group">
      <label for="titleId">Title</label>
      <input type="text" name="titleData" id="titleId" class="form-control">
    </div>
    <div class="form-group">
      <label for="ostId">Post</label>
      <textarea name="postData" id="postId" cols="30" rows="5" class="form-control"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
  </form>
<%- include("partials/footer"); -%>